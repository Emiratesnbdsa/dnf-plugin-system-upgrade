.multi-button { height: 48px; max-height: 48px; box-sizing: border-box; display: inline-flex; position: relative; }

.multi-button > a { background-color: rgb(255, 255, 255); outline: rgb(197, 192, 192) solid 1px; border-width: 0px 0px 3px; border-top-style: initial; border-right-style: initial; border-left-style: initial; border-top-color: initial; border-right-color: initial; border-left-color: initial; border-image: initial; border-bottom-style: solid; border-bottom-color: rgba(0, 0, 0, 0); border-radius: 0px; box-sizing: border-box; color: rgb(21, 4, 4); cursor: pointer; display: flex; font-size: 16px; height: 100%; margin: 0px; min-width: 200px; overflow: hidden; padding: 12px 38px 12px 16px; text-align: left; transition: border-color 0.2s ease 0s, outline-color 0.2s ease 0s; white-space: nowrap; width: 100%; text-decoration: none; }

.multi-button > a::after { content: ""; display: block; background-image: url("https://webobjects2.cdw.com/is/content/CDW/cdw/on-domain-cdw/cdw-branded/cdw-icons/down-triangle.svg"); background-repeat: no-repeat; background-size: contain; height: 9px; pointer-events: none; position: absolute; right: 14px; top: 50%; transform: translateY(-50%); width: 12px; }

.multi-button > a:hover, .multi-button > a:active, .multi-button > a:focus, .multi-button > a.active, .multi-button > a:focus-visible { color: rgb(21, 4, 4); border-bottom: 3px solid rgb(21, 4, 4); outline: rgb(21, 4, 4) solid 1px; outline-offset: unset; }

.multi-button > a.active::after { transform: translateY(-50%) rotate(180deg); }

.multi-button > a.-disabled { cursor: default; opacity: 0.5; pointer-events: none; }

.multi-button select { display: none; }

